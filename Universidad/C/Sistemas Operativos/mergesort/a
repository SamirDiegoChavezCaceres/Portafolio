fprintf('1. Utilizando la función difflim.m descrita en el marco teórico, calcular:')
difflim('sin', 1, 1)
fprintf('2. Utilizando la función difflim.m descrita en el marco teórico, calcular:')
difflim('sin', 1, 5)
fprintf('3. Utilizando la función difflim.m descrita en el marco teórico, calcular:')
difflim('sin', 1, 10)
fprintf('4. Utilizando la función difflim.m descrita en el marco teórico, calcular:')
difflim('exp', 1, 1)
fprintf('5. Utilizando la función difflim.m descrita en el marco teórico, calcular:')
difflim('exp', 1, 5)
fprintf('6. Utilizando la función difflim.m descrita en el marco teórico, calcular:')
difflim('exp', 1, 10)
fprintf('7. Utilizando la función difflim.m descrita en el marco teórico, para esto se le pide declarar la función anónima f=@(x) 60*x.^45, calcular:')
f=@(x) 60*x.^45
difflim(f,1,1)
fprintf('8. Utilizando la función difflim.m descrita en el marco teórico, para esto se le pide declarar la función anónima f=@(x) 60*x.^45, calcular:')
difflim(f,1,5)
fprintf('9. Utilizando la función difflim.m descrita en el marco teórico, para esto se le pide declarar la función anónima f=@(x) 60*x.^45, calcular:')
difflim(f,1,10)
fprintf('10. Utilizando a función diffext.m descrita en el marco teórico, calcular:')
 diffext ('sin', 1, 1, 1)
fprintf('11. Utilizando la función diffext.m descrita en el marco teórico, calcular:')
 diffext ('sin', 1, 1, 5)
fprintf('12. Utilizando la función diffext.m descrita en el marco teórico, calcular:')
 diffext ('sin', 1, 1, 10)
fprintf('13. Utilizando la función difflim.m descrita en el marco teórico, calcular:')
 diffext ('exp', 1, 1, 1)
fprintf('14. Utilizando la función difflim.m descrita en el marco teórico, calcular:')
 diffext ('exp', 1, 5, 1)
fprintf('15. Utilizando la función difflim.m descrita en el marco teórico, calcular:')
 diffext ('exp', 1, 10, 1)
fprintf('16. Utilizando la función diffext.m descrita en el marco teórico, para esto se le pide declarar la función anónima f=@(x) 60*x.^45, calcular:')
 diffext (f,1,1,1)
fprintf('17. Utilizando la función diffext.m descrita en el marco teórico, para esto se le pide declarar la función anónima f=@(x) 60*x.^45, calcular:')
 diffext (f,1,5,1)
fprintf('18. Utilizando la función diffext.m descrita en el marco teórico, para esto se le pide declarar la función anónima f=@(x) 60*x.^45, calcular:')
diffext(f,1,10,1)
fprintf('19. Utilizando la función diff de matlab encontrar la derivada de la función sin(5*x)')
syms x; f = sin(5*x);
diff(f)
fprintf('20. Utilizando la función diff de matlab encontrar la derivada de la función g:')
g = exp(x)*cos(x);
diff(g)
fprintf('21. Utilizando la función diff de matlab encontrar la derivada de la función g = exp(x)*cos(x)')
diff(g,2)
fprintf('22. Utilizando la función diff de matlab encontrar la derivada de la función g = exp(x)*cos(x)')
diff(diff(g))
fprintf('23. Utilizando la función diff de matlab encontrar la derivada de la función c = 5')
c = sym('5');
diff(c)
fprintf('24. Utilizando la función diff de matlab encontrar la derivada de la función c = 5')
diff(5)
fprintf('25. Utilizando la función diff de matlab encontrar la derivada de la función f = sin(s*t) en relación de t')
syms s t; f = sin(s*t);
diff(f,t)
fprintf('26. Utilizando la función diff de matlab encontrar la derivada de la función f = sin(s*t) en relación de s')
diff(f,s)
fprintf('27. Utilizando la función diff de matlab encontrar la derivada de la función f = sin(s*t)')
diff(f)
fprintf('28. Utilizando la función diff de matlab encontrar la derivada de la función f = x ^n')
syms x n; f = x ^n;
 diff(f)
fprintf('29. Utilizando la función diff de matlab encontrar la derivada de la función f= sin(a*t + b)')
syms a b t; f = sin(a*t + b);
diff(f)
fprintf('30. Utilizando la función diff de matlab encontrar la derivada de la función f=exp(i* theta)')
syms theta; f = exp(1i*theta);
diff(f)